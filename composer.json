{
  "name": "spicyweb/craft-block-batch-actions",
  "description": "A Craft CMS plugin for performing batch actions on Matrix and Neo blocks",
  "version": "0.1.0",
  "type": "craft-plugin",
  "keywords": [
    "cms",
    "craftcms",
    "plugin",
    "neo",
    "matrix",
    "field"
  ],
  "license": "MIT",
  "authors": [
    {
        "name": "Spicy Web",
        "email": "plugins@spicyweb.com.au",
        "homepage": "https://spicyweb.com.au/"
    }
  ],
  "support": {
    "issues": "https://github.com/spicywebau/craft-block-batch-actions/issues",
    "source": "https://github.com/spicywebau/craft-block-batch-actions",
    "docs": "https://github.com/spicywebau/craft-block-batch-actions/blob/1.0.0/README.md",
    "rss": "https://github.com/spicywebau/craft-block-batch-actions/commits/1.x.atom"
  },
  "require": {
    "craftcms/cms": "^4.2.0",
    "php": "^8.0.2"
  },
  "suggest": {
    "spicyweb/craft-neo": "Supports Neo 3.3.4 or later"
  },
  "autoload": {
    "psr-4": {
        "spicyweb\\blockbatchactions\\": "src/"
    }
  },
  "extra": {
    "handle": "blockbatchactions",
    "name": "Block Batch Actions",
    "class": "spicyweb\\blockbatchactions\\Plugin",
    "changelogUrl": "https://github.com/spicywebau/craft-block-batch-actions/blob/1.x/CHANGELOG.md",
    "downloadUrl": "https://github.com/spicywebau/craft-block-batch-actions/archive/refs/tags/1.0.0.zip"
  }
}
